{
  "name": "imjoy",
  "version": "0.11.3",
  "private": false,
  "description": "ImJoy -- deep learning made easy.",
  "author": "imjoy-team <imjoy.team@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjoy-team/ImJoy.git"
  },
  "keywords": [
    "ImJoy",
    "PWA",
    "Deep Learning"
  ],
  "bugs": {
    "url": "https://github.com/imjoy-team/ImJoy/issues"
  },
  "homepage": "https://imjoy.io",
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "check-format": "prettier --check \"{src,tests}/**/**\"",
    "format": "prettier --write \"{src,tests}/**/**\"",
    "lint": "vue-cli-service lint",
    "check": "prettier --check \"{src,tests}/**/**\" && vue-cli-service lint && vue-cli-service test:unit",
    "deploy": "DEPLOY_MODE=dev vue-cli-service build && node ./deploy-dev.js"
  },
  "dependencies": {
    "ajv": "^6.9.1",
    "axios": "^0.18.1",
    "dompurify": "^2.0.8",
    "file-saver": "^1.3.3",
    "imjoy-core": "0.13.8",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "marked": "^0.8.0",
    "minibus": "^3.1.0",
    "monaco-editor": "^0.15.6",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "pouchdb-browser": "^7.0.0",
    "register-service-worker": "^1.6.2",
    "spark-md5": "^3.0.0",
    "spectre.css": "^0.5.8",
    "vega": "^5.7.3",
    "vega-embed": "^6.0.0",
    "vega-lite": "^4.0.0-beta.10",
    "vue": "^2.5.17",
    "vue-form-generator": "^2.3.4",
    "vue-grid-layout": "github:jbaysolutions/vue-grid-layout#9e13ea9",
    "vue-js-modal": "github:imjoy-team/vue-js-modal#2.0.0-rc.3g",
    "vue-material": "^1.0.0-beta-12",
    "vue-router": "^3.0.1",
    "xterm": "^3.13.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-plugin-unit-mocha": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.0-beta.32",
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "chromedriver": "^2.38.2",
    "cname-webpack-plugin": "^1.0.3",
    "copy-webpack-plugin": "^4.6.0",
    "create-file-webpack": "^1.0.2",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.3",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^10.0.8",
    "mocha": "^5.0.5",
    "mocha-webpack": "^2.0.0-beta.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "prettier": "1.17.0",
    "raw-loader": "^1.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^2.6.2",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "uglify-js": "^2.8.29",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-unit-karma": "^0.5.0",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/**": "prettier --write"
  }
}
